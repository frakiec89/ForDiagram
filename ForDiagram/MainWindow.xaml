        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ForDiagram"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="5">
            <Button x:Name="Add" Click="Add_Click" >
                Добавить
            </Button>


            <StackPanel Margin="5" x:Name="stak_main">
            </StackPanel>

            <Button x:Name="runExel" Click="runExel_Click" >
                Получить  с  exel
            </Button>
            
            <Button x:Name="run" Click="run_Click" >
                Посмотреть
            </Button>



            <ListBox Margin="5" x:Name="lbResault"></ListBox>
        </StackPanel>
        
        

    </Grid>
</Window>
